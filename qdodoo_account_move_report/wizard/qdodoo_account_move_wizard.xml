<?xml version='1.0' encoding="utf-8"?>
<openerp>
    <data>
        <record id="qdodoo_account_move_wizard_form" model="ir.ui.view">
            <field name="name">批量打印凭证</field>
            <field name="model">qdodoo.account.move.wizard</field>
            <field name="arch" type="xml">
                <form string="批量打印凭证">
                    <group>
                        <separator string="你确定要批量打印这些凭证吗?"/>
                    </group>
                    <footer>
                        <button string="打印" name="report_print" type="object" class="oe_highlight"/>
                        or
                        <button string="取消" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <act_window name="批量打印凭证"
        res_model="qdodoo.account.move.wizard"
        src_model="account.move"
        view_mode="form"
        target="new"
        multi="True"
        key2="client_action_multi"
        id="qdodoo_account_move_wizard2"/>

    </data>
</openerp>