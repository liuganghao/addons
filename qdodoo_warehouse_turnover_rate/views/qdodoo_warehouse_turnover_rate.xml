<?xml version="1.0" ?>
    <openerp>
        <data>
<!--周转率form视图-->
            <record model="ir.ui.view" id="qdodoo_ratesearch_form">
                <field name="name">qdodoo.ratesearch.form</field>
                <field name="model">qdodoo.warehouse.ratesearch</field>
                <field name="arch" type="xml">
                    <form string="beriyou data">
                            <group>
                                <group>
                                    <field name="from_date"/>
                                    <field name="location_id"/>
                                </group>
                                <group>
                                    <field name="to_date"/>
                                </group>
                            </group>
                            <footer>
                                <button name="action_open_new_window" string="查看周转率" type="object" class="oe_highlight"/>
                                or
                                <button string="取消" class="oe_link" special="cancel"/>
                            </footer>

                    </form>
                </field>
            </record>




<!--模型按钮-->
        <act_window name="库存周转率"
                    res_model="qdodoo.warehouse.ratesearch"
                    src_model="product.product"
                    view_mode="form"
                    target="new"
                    multi="True"
                    key2="client_action_multi"
                    id="action_qdodoo_warehouse_ratesearch"/>




<!--周转率form模型-->
            <record model="ir.ui.view" id="qdodoo_turnrate_form">
                <field name="name">qdodoo.turnrate.form</field>
                <field name="model">qdodoo.warehose.turnrate</field>
                <field name="arch" type="xml">
                    <form string="beriyou data">
                        <field name="product"/>
                        <field name="ware_name" />
                        <group>
                            <group >
                                <field name="start_date" />
                                <field name="end_date" />
                                <field name="ware_name"/>
                            </group>
                            <group>
                            <field name="beginning" />
                            <field name="final" />
                            <field name="number" />
                            <field name="turnover_rate" widget="progressbar"/>
                            </group>
                        </group>

                    </form>
                </field>
            </record>
<!--周转率tree视图-->
            <record model="ir.ui.view" id="qdodoo_turnrate_tree">
                <field name="name">qdodoo.turnrate.tree</field>
                <field name="model">qdodoo.warehose.turnrate</field>
                <field name="arch" type="xml">
                    <tree>
                        <field name="product"/>
                        <field name="ware_name"/>
                        <field name="beginning" />
                        <field name="final" />
                        <field name="number" />
                        <field name="turnover_rate"  widget="progressbar"/>
                    </tree>
                </field>
            </record>
        </data>
</openerp>
